{
  "name": "paybutton-docs",
  "private": "true",
  "version": "1.0.0",
  "description": "PayButton Documentation",
  "type": "module",
  "scripts": {
    "update-pb": "npm i @paybutton/paybutton @paybutton/react",
    "update-version": "npm ls @paybutton/paybutton --json | jq '.dependencies[\"@paybutton/paybutton\"].version' -rj > VERSION",
    "start": "cross-env GATSBY_PAYBUTTON_VERSION=\"$(cat VERSION)\" docz dev",
    "prebuild": "npm run update-pb",
    "build": "cross-env GATSBY_PAYBUTTON_VERSION=\"$(cat VERSION)\" docz build"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@material-ui/styles": "^4.10.0",
    "@mdx-js/mdx": "^1.6.16",
    "@paybutton/paybutton": "^1.0.4",
    "@paybutton/react": "^1.0.5",
    "docz": "^2.3.1",
    "gatsby-theme-docz": "^2.3.1",
    "react": "^16.13.1",
    "react-codemirror2": "^7.2.1",
    "react-datepicker": "^3.2.2",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.7"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
